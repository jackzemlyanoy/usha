.logo
	display inline-block
	width: 124px
	height: 20px
	text-decoration none
	transition all .4s linear
	svg
		transition all .4s linear
		width: 100%
		height: 100%
		fill $asphalt
	.subMenu-mobile &
		display: inline-block!important
	.header_scrolled &
		+below(992px)
			display: none
			
			
	.header_scrolled &
		width: 124px - 15%
		height: 20px - 15%
	
	&__img
		display block
		max-width 100%
	
	&_inner
		width: 78px